<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    android:id="@+id/photo_cardview"
    android:layout_width="200dp"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_margin="10dp"
    app:cardCornerRadius="@dimen/default_cardview_corner_radius"
    android:elevation="@dimen/default_cardview_elevation"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rover_photo_imageview"
        android:contentDescription="@string/rover_photo_cd"
        tools:src="@drawable/photos_1"
        android:scaleType="centerCrop"
      />




</android.support.v7.widget.CardView>